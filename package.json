{
  "name": "Rootandleaves",
  "description": "Modern Multipurpose Landing Pages Template Using React, Next, Styled Components and so on. It also support Gatsby Static site generator ",
  "version": "2.0.0",
  "private": true,
  "homepage": "http://SarthakAditya.github.io/rootandleaves",
  "workspaces": [
    "packages/common",
    "packages/landing",
    "packages/reusecore"
  ],
  "scripts": {
    "clean": "lerna clean",
    "del-dist": "lerna exec --parallel --rimraf public",
    "start": "next start",
    "now-build": "next build",
    "dev": "next",
    "build": "next build",
    "core": "cd ./packages/common && yarn dev",
    "core-build": "cd ./packages/common && yarn build",
    "format": "prettier --trailing-comma es5 --single-quote --write 'packages/*/*.js' 'packages/*/!(node_modules)/**/*.js'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@glidejs/glide": "^3.2.4",
    "@redq/reuse-modal": "^1.1.9",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "formBuilder": "^3.2.4",
    "gh-pages": "^2.0.1",
    "jquery": "^3.4.1",
    "next": "^8.0.0-canary.15",
    "next-compose-plugins": "^2.1.1",
    "next-fonts": "^0.17.0",
    "next-optimized-images": "^2.3.3",
    "next-transpile-modules": "^2.0.0",
    "now": "^15.3.0",
    "popper.js": "^1.15.0",
    "prop-types": "^15.6.2",
    "rc-drawer": "^1.7.6",
    "rc-progress": "^2.3.0",
    "rc-tabs": "^9.5.6",
    "react": "^16.8.2",
    "react-accessible-accordion": "^2.4.5",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-content-loader": "^4.0.1",
    "react-countup": "^4.0.0",
    "react-dom": "^16.8.2",
    "react-icons-kit": "^1.2.1",
    "react-image-gallery": "^0.8.12",
    "react-particles-js": "^2.4.2",
    "react-reveal": "^1.2.2",
    "react-rnd": "^9.0.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.1",
    "react-scroll-parallax": "^1.3.5",
    "react-scrollspy": "^3.3.5",
    "react-stickynode": "^2.1.0",
    "styled-components": "^4.1.1",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "docz": "^0.13.7",
    "husky": "^1.1.4",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "lerna": "^3.4.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.3"
  }
}
